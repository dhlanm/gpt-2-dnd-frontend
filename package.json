{
  "name": "gpt-2-dnd-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.0.4",
    "dirty-json": "^0.8.0",
    "html2canvas": "^1.0.0-rc.5",
    "promise-polyfill": "^8.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typescript": "3.8.3",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix ./src --ext .ts,.tsx,.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "!.eslintrc.js",
    "!.prettierrc.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^5.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  }
}
